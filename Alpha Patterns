#include <iostream>
using namespace std;
int main(int argc, char const *argv[])
{int n;
    cin>>n;
    int i=0; //or i=1 lega to char ch = 'A'=i-1   
     while(i<n){
        int j=1;    
        while (j<=n)    
        {
            char ch= 'A'+i;
        cout<< ch<<" ";  
       
        j=j+1;
        
        } 
        cout<<endl;
        i=i+1;
        
    }
    return 0;
}
  
  //2nd pattern 

